            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <TextField fx:id="publisherPublisher_field" GridPane.columnIndex="1" />
                <TextField fx:id="publisherCity_field" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Label text="Название">
                    <font>
                        <Font size="17.0" />
                    </font>
                </Label>
                <Label text="Город" GridPane.rowIndex="1">
